<%- include('partials/header'); -%>
<h1>Compose</h1>
<br>
<form class="" action="" method="post">
  <div class="form-group">
    <label for="blogTitle">Title</label>

    <input class="form-control" type="text" name="blogTitle" placeholder="Choose a catchy one" autocomplete="off">

    <label for="blogContent">Post</label>

    <textarea name="blogContent" class="form-control" rows="8" cols="80" autocomplete="off" placeholder="Change the world with your Blog!!"></textarea>
  </div>


  <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>
<%- include('partials/footer'); -%>
